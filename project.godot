; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="Car(d) Race"
run/main_scene="res://_src/Menus/Start/Start.tscn"
boot_splash/image="res://title_x4.png"
boot_splash/fullsize=false
config/icon="res://icon.png"

[autoload]

Global="*res://_src/Global.gd"

[display]

window/size/width=1280
window/size/height=720
window/size/test_width=1920
window/size/test_height=1080
window/vsync/use_vsync=false
window/stretch/mode="2d"
window/stretch/aspect="keep"
window/stretch/shrink="1"

[layer_names]

2d_physics/layer_1="Base"
2d_physics/layer_2="Cars"
2d_physics/layer_3="Cards"

[rendering]

quality/2d/use_pixel_snap=true
environment/default_environment="res://default_env.tres"
